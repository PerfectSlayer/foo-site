#parse( "default-site-macros.vm" )
<!-- Generated by Apache Maven Doxia Site Renderer#if( $doxiaSiteRendererVersion ) $doxiaSiteRendererVersion#end at $dateFormat.format( $currentDate ) -->
<!-- Modified just for fun -->
<html xmlns="http://www.w3.org/1999/xhtml"#if ( $locale ) xml:lang="$locale.language" lang="$locale.language"#end>
<head>

    <meta http-equiv="Content-Type" content="text/html; charset=${outputEncoding}" />
    
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Site web du Paris Java user groups">
    <meta name="author" content="">
    <link href="/css/sticky-footer-navbar.css" rel="stylesheet">
    <link href="/css/offcanvas.css" rel="stylesheet">
    <link href="/css/maven-theme.css" rel="stylesheet">
    <link href="/css/jug-theme.css" rel="stylesheet">
    <link href="/css/sticky-footer-navbar.css" rel="stylesheet">
    <link rel="icon" href="../../../../favicon.ico">

    <title>$title </title>
    #foreach( $author in $authors )
        <meta name="author" content="$author" />
    #end
    #if ( $documentDate )
        <meta name="date" content="$documentDate" />
    #end
    #if ( $locale )
        <meta http-equiv="Content-Language" content="$locale.language" />
    #end
    #if( $decoration.body.head )
        $render.eval( $decoration.body.head )
    #end
    $headContent
    #googleAnalytics( $decoration.googleAnalyticsAccountId )
   
  </head>

 
</head>
<body class="composite">



<nav class="navbar navbar-default">
      <div class="container">
        
        <!-- start -->
        #mainMenu( $decoration.body.menus )
        <!-- end -->
        </div><!--/.nav-collapse -->
      </div>
    </nav>
<nav class="navbar navbar-default">
      <div class="container">
        
        <!-- start -->
        <!-- @ToDO make it dynamic -->
        <div class="col-sm-3">
          <img src="/images/sponsors/2017/arolla.svg" class="bannerPlatine" alt="Arolla">
        </div>
                <div class="col-sm-3">
          <img src="/images/sponsors/2017/zenika.svg" class="bannerPlatine" alt="Arolla">
        </div>
                <div class="col-sm-3">
          <img src="/images/sponsors/2017/carbon-IT.svg" class="bannerPlatine" alt="Arolla">
        </div>
                <div class="col-sm-3">
          <img src="/images/sponsors/2017/couchbase.svg" class="bannerPlatine" alt="Arolla">
        </div>
                <div class="col-sm-3">
          <img src="/images/sponsors/2017/mirakl.svg" class="bannerPlatine" style="width: 300px" alt="Arolla">
        </div>
                <div class="col-sm-3">
          <img src="/images/sponsors/2017/norsys.svg" class="bannerPlatine" style="width: 300px" alt="Arolla">
        </div>
                <div class="col-sm-3">
          <img src="/images/sponsors/2017/oxiane.svg" class="bannerPlatine" alt="Arolla">
        </div>
                <div class="col-sm-1">
          <img src="/images/sponsors/2017/ippon.svg" class="bannerPlatine" alt="Arolla">
        </div>
                <div class="col-sm-1">
          <img src="/images/sponsors/2017/viseo.svg" class="bannerPlatine" alt="Arolla">
        </div>
        <!-- end -->
        </div><!--/.nav-collapse -->
      </div>
    </nav>
<div id="bodyColumn">
    <div id="contentBox">
        $bodyContent
    </div>
</div>
<div class="clear">
    <hr/>
</div>
 <footer class="footer">
      <div class="container">
      <span class="text-muted">
      <span class="">
       #poweredByLogo( $decoration.poweredBy )
       </span>
       <span >
        Copyright &#169;#copyright()All rights reserved.
       </span></br>
       <span>#publishDate( "bottom" $decoration.publishDate $decoration.version )</span>
        <img src="http://mirrors.creativecommons.org/presskit/buttons/88x31/png/by-nc.eu.png" style="width: 40px;">
        
        </span>
      </div>
    </footer>
    
<div id="footer">
    <div class="xright">
        
    </div>
    <div class="clear">
        <hr/>
    </div>
</div>
</body>
